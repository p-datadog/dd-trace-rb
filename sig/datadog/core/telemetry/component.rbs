module Datadog
  module Core
    module Telemetry
      class Component
        @enabled: bool
        @stopped: bool
        @metrics_manager: Datadog::Core::Telemetry::MetricsManager
        @worker: Datadog::Core::Telemetry::Worker

        attr_reader enabled: bool

        include Core::Utils::Forking

        def initialize: (heartbeat_interval_seconds: Float, metrics_aggregation_interval_seconds: Float, dependency_collection: bool, ?enabled: bool, ?metrics_enabled: bool) -> void

        def disable!: () -> void

        def client_configuration_change!: (Enumerable[[String, Numeric | bool | String]] changes) -> void

        def emit_closing!: () -> void

        def stop!: () -> void

        def integrations_change!: () -> void
      end
    end
  end
end
